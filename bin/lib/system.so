import "Console";

print(message) => {
	"Console.PRINT" _OUTER_CALL_ (str message);
};

println(message) => {
	print(str message + "\n");
};

exit() => {
	"Console.EXIT" _OUTER_CALL_ null;
};

static Input class {
	line() => {
		return ("Console.GET.LINE" _OUTER_CALL_ null);
	};
	number() => {
		return ("Console.GET.NUMBER" _OUTER_CALL_ null);
	};
};

String::match(string, pattern) => {
	return "Console.REGEX" _OUTER_CALL_ { pattern, string };
};