load "map.so";

var m = new Map();

var start = timeMS();

i in {0 until 1000} do {
	m[str Random.int(0, 1000)] = Random.float();
}

println("Map Object:");
println("Time: " + str (timeMS() - start) + "ms");
println("Size: " + str m.length());

var dict = @{};

start = timeMS();

i in {0 until 1000} do {
	dict[str Random.int(0, 1000)] = Random.float();
}

println("Dictionary:");
println("Time: " + str (timeMS() - start) + "ms");
println("Size: " + str dict.length());