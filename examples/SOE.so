SOE(final MAX) => {
	var a = alloc(MAX);
	e in a do {
		e = true;
	}

    var c = MAX - 2;
	i in {2 until (MAX ** 0.5)} do {
		a[i] then {
			var n = i ** 2;
			n < MAX do {
			    a[n] then {
			        c -= 1;
					a[n] = false;
			    }
				n += i;
			}
		}
	}

	var b = alloc(c);
	var j = 0;
	i in {2 until MAX} do {
		a[i] then {
			b[j] = i;
			j += 1;
		}
	}

	return b;
}

var MAX = 10 ** 6;

var start = timeMS();
var result = SOE(MAX);
var time = timeMS() - start;

println(str len result + " Primes found for numbers below " + str MAX);
println("Time: " + str time + "ms");