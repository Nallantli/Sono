load "system";
load "socket";

var client = new Socket("127.0.0.1", 5000);

var output = client.getOutput();
var input = client.getInput();

var line;

(line = Input.line()) != "" do {
	print("Client Sending:\t" + line);
	output.send(line);
	print("Client Recieved:\t" + input.read());
};

output.close();
input.close();
client.close();